<h1>Listing user_apps</h1>

<table>
  <tr>
    <th>User</th>
    <th>App</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_apps.each do |user_app| %>
  <tr>
    <td><%= user_app.user_id %></td>
    <td><%= user_app.app_id %></td>
    <td><%= link_to 'Show', user_app %></td>
    <td><%= link_to 'Edit', edit_user_app_path(user_app) %></td>
    <td><%= link_to 'Destroy', user_app, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

